config {
  type: "table",
  schema: "bi_dashboard",
  name: "bi_statements",
  description: "tabela final de pontuacao",
  tags: ["bi", "statements","pontuacao"]
}


SELECT *,
 SUM(Pontos) OVER (
    ORDER BY DATETIME(Data), Id_Transacao
    ROWS BETWEEN UNBOUNDED PRECEDING AND CURRENT ROW
  ) AS Saldo_Acumulado
FROM ${ref("op_pontuacao_join_1")}